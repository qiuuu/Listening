    android:layout_width="match_parent"
    android:layout_height="match_parent"

    tools:context="com.pangge.listenstudy.MainActivity">



    <FrameLayout

        android:id="@+id/book_fragment"
        android:name="com.example.iuuu.listenstudy.BookFragment"
        android:layout_width="match_parent"
